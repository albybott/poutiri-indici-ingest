{
  "name": "poutiri-indici-ingest",
  "version": "1.0.0",
  "description": "Headless S3â†’Postgres ingestion for Indici extracts (raw/stg/core with audits).",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=9.5.0"
  },
  "packageManager": "pnpm@9.5.0",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "dev:debug": "tsx --inspect --watch src/index.ts",
    "build": "tsc",
    "build:clean": "pnpm clean && pnpm build",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "check-all": "pnpm type-check && pnpm lint && pnpm format:check && pnpm test:run"
  },
  "keywords": [
    "typescript",
    "node",
    "etl",
    "s3",
    "postgres"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.19.17",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.44.5",
    "pg": "^8.16.3",
    "postgres": "^3.4.7"
  }
}
